    "mocha": "^8.3.2",
    "power-assert": "^1.6.1",
    "ts-loader": "^9.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tscpaths": "0.0.9",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
