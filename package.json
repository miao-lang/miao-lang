  },
  "homepage": "https://github.com/miao-lang/miao-lang#readme",
  "dependencies": {
    "js-base64": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/assert": "^1.5.6",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "babel-loader": "^8.2.3",
