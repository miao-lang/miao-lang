{
  "name": "miao-lang",
  "version": "1.0.5",
  "description": "A language of Miao.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:0": "cross-env NODE_ENV=production webpack --config scripts/webpack.main.prod.config.js",
